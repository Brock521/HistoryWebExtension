{
    "manifest_version": 2,
    "name": "Your Extension",
    "version": "1.0",
    "permissions": [
      "activeTab","tabs","webRequest", "webRequestBlocking","http://localhost:8080/*"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Click me"
      },
    "background": {
        "scripts": ["background.js"]
      },
      "web_accessible_resources": [
          "index.html",
          "index.css"
        ]
}
  